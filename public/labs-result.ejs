<section class="labs gap-bottom-xsmall">

    <div class="row headline">
        <div class="columns">
            <h1 class="size-large gap-top-medium gap-bottom-xgit stxsmall">
                Timesheet.xlsx
            </h1>
        </div>
    </div>

    <div class="row">
        <div class="small-12 columns">
            <a href="/download" class="btn size-medium bg-blue bg-lighten text-white shadow">
                Copy configuration
            </a>
            &nbsp;
            <a href="/download" class="btn size-medium bg-green bg-lighten text-white shadow">
                Edit in JSFiddle
            </a>

        </div>
    </div>

    <div class="row">
        <div class="small-12 columns">
            <div class="actions text-right">
                <a href="/labs-upload.html" target="_blank" rel="noopener" class="new">Create new</a>
                <a href="#" target="_blank" rel="noopener" class="remove">Delete</a>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="small-12 columns">
            <div class="tabs tabs-sheets">
                <div id="sheet1" class="active">
                    <div class="plugin-preview">
                        <div id="hot"></div>
                    </div>
                </div>
                <div id="sheet2">
                    <div class="row">
                        <div class="small-12 columns">
                            Handsontable Sheet 2
                        </div>
                    </div>
                </div>
                <div id="sheet3">
                    <div class="row">
                        <div class="small-12 columns">
                            Handsontable Sheet 3
                        </div>
                    </div>
                </div>
                <div class="tab">
                    <ul>
                        <li id="tab-sheet1" class="active">Sheet1</li>
                        <li id="tab-sheet2">Sheet2</li>
                        <li id="tab-sheet3">Sheet3</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div class="row gap-top-xsmall">
        <div class="small-12 medium-3 columns">
            <div class="stats">
                <h3>Overview</h3>
                <p>Created on May 16, 2017</p>
                <p>Worksheets: 4</p>
                <p>Non-blank cells: 25</p>
                <p>Formulas: 25</p>
            </div>
        </div>
        <div class="small-12 medium-3 end columns">
            <div class="report">
                <h3>Help us improve</h3>
                <a href="//handsoncode.typeform.com/to/NeUbrn" target="_blank" rel="noopener noreferrer">
                    Fill out this quick survey</a>
            </div>
        </div>
        <div class="small-12 medium-5 end columns">
            <div class="share">
                <h3>Share link</h3>
                <input type="text" id="share-url" value="https://labs.handsontable.com/convert/013081380180"
                       onclick="this.select();" readonly />
            </div>
        </div>
    </div>

    <div class="row comments gap-top-small" id="comments">
        <div class="large-8 medium-12 large-centered columns">
            <!-- START EMBED CODE -->
            <div id="disqus_thread"></div>
            <script>
                var disqus_config = function () {
                    this.page.url = handsontable.com;
                    this.page.identifier = handsontable-labs;
                };
                (function () { // DON'T EDIT BELOW THIS LINE
                    var d = document, s = d.createElement('script');

                    s.src = '//handsontable.disqus.com/embed.js';

                    s.setAttribute('data-timestamp', +new Date());
                    (d.head || d.body).appendChild(s);
                })();
            </script>
            <noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript" rel="nofollow">
                    comments powered by Disqus.</a></noscript>
            <!-- END EMBED CODE -->
        </div>
    </div>
</section>
