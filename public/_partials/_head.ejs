<title><%= brand %> - <%= title %></title>
<meta charset="utf-8">
<meta name=viewport content="width=device-width, initial-scale=1">
<meta name="description" content="<%= description %>">

<% if (current.source === 'pricing-maintenance') {  %>
    <meta name="robots" content="noindex" />
<% } %>

<link href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700" rel="stylesheet">

<% if (current.source === 'index') {  %>
  <!-- build:css /static/css/optimized-index.css -->
  <link href="/static/css/main.css" type="text/css" rel="stylesheet">
  <!-- /build -->
<% } else if (current.source === 'pricing') {  %>
  <!-- build:css /static/css/optimized-pricing.css -->
  <link href="/static/css/main.css" type="text/css" rel="stylesheet">
  <!-- /build -->
<% } else { %>
  <link href="/static/css/main.css" type="text/css" rel="stylesheet">
<% } %>

<script>
    (function(d) {
        var config = {
            kitId: 'koz1efc',
            scriptTimeout: 3000,
            async: false
        },
          h=d.documentElement,t=setTimeout(function(){h.className=h.className.replace(/\bwf-loading\b/g,"")+" wf-inactive";},config.scriptTimeout),tk=d.createElement("script"),f=false,s=d.getElementsByTagName("script")[0],a;h.className+=" wf-loading";tk.src='https://use.typekit.net/'+config.kitId+'.js';tk.async=true;tk.onload=tk.onreadystatechange=function(){a=this.readyState;if(f||a&&a!="complete"&&a!="loaded")return;f=true;clearTimeout(t);try{Typekit.load(config)}catch(e){}};s.parentNode.insertBefore(tk,s)
    })(document);
</script>

<script>
  var PAGE_NAME = '<%= current.source %>';
  
  if (typeof fetch === 'undefined') {
      document.write('<scr'+'ipt src="/static/bower_components/fetch/fetch.js"></scr'+'ipt>');
  }
  if (typeof Promise === 'undefined') {
      document.write('<scr'+'ipt src="/static/bower_components/promise-polyfill/promise.min.js"></scr'+'ipt>');
  }
  if (!document.createElement('div').dataset) {
    document.write('<scr'+'ipt src="/static/js/lib/html5-dataset.js"></scr'+'ipt>');
  }
  if (!document.createElement('div').classList) {
    document.write('<scr'+'ipt src="/static/node_components/classlist-polyfill/index.js"></scr'+'ipt>');
  }
</script>

<script async defer src="/static/js/common/svgGetter.js"></script>

<% if (current.source === 'examples'|| current.path[1] == 'articles' || current.source === 'features' || current.source === 'labs-result' || current.source === 'community-download' || current.source === 'pro-download') {  %>
    <link href="/static/bower_components/handsontable-pro/dist/handsontable.full.min.css" type="text/css" rel="stylesheet">
    <link href="/static/bower_components/highlightjs/styles/github.css" type="text/css" rel="stylesheet">
    <link href="/static/js/lib/prism.min.css" type="text/css" rel="stylesheet">

    <% if (current.source === 'features') {  %>
      <link href="/static/node_components/codemirror/codemirror.css" type="text/css" rel="stylesheet">
      <link href="/static/node_components/codemirror/theme/dracula.css" type="text/css" rel="stylesheet">
    <% } %>
<% } %>