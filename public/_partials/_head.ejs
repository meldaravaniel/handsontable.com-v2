<script src="/static/js/common/index.js"></script>

<% if (current.source === 'examples'|| current.path[1] == 'articles' || current.source === 'features' || current.source === 'labs-result' || current.source === 'community-download' || current.source === 'pro-download') {  %>
    <link href="/static/bower_components/handsontable-pro/dist/handsontable.full.css" type="text/css" rel="stylesheet">
    <link href="/static/bower_components/highlightjs/styles/github.css" type="text/css" rel="stylesheet">
    <link href="/static/js/lib/prism.css" type="text/css" rel="stylesheet">

    <script src="/static/bower_components/handsontable-pro/dist/handsontable.full.js"></script>
    <script src="/static/bower_components/history.js/scripts/bundled/html4+html5/native.history.js"></script>
    <script src="/static/bower_components/highlightjs/highlight.pack.min.js"></script>
    
    <% if (current.source === 'features') {  %>
      <link href="/static/node_components/codemirror/codemirror.css" type="text/css" rel="stylesheet">
